{
    "compilerOptions": {
      "outDir": "./public/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react-jsx",
      "jsxImportSource": "preact",
      "allowJs": true,
      "moduleResolution": "node",
      "paths": {
        "react": ["./node_modules/preact/compat/"],
        "react-dom": ["./node_modules/preact/compat/"]
      }
    }
  }